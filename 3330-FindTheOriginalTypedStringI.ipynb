   "source": [
    "from typing import *\n",
    "\n",
    "class Solution:\n",
    "    def possibleStringCount(self, word: str) -> int:\n",
    "        output = 1\n",
    "        for i in range(1, len(word)):\n",
    "            if word[i-1] == word[i]: output += 1 \n",
    "        return output"
   ]
