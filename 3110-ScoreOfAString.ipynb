    "from typing import *\n",
    "\n",
    "class Solution:\n",
    "    def scoreOfString(self, s: str) -> int:\n",
    "        score = 0\n",
    "        for i in range(1, len(s)):\n",
    "            score += abs(ord(s[i])-ord(s[i-1]))\n",
    "        return score"
   ]
